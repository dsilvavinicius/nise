{
    "description": "SIREN Learning of the Armadillo mesh.",
    "space_time": true,
    "num_epochs": 20001,
    "sampling_opts": {
        "samples_on_surface": 82000,
        "samples_off_surface": 82000,
        "random_surf_samples": true,
        "sampler": false
    },
    "dataset": [["armadillo.ply", "shapeNets/armadillo-2x128_w0-30.pth", 0]],
    "batch_size": 40000,
    "checkpoint_path": "logs",
    "experiment_name": "armadillo_2x128_w-30_GPNF_tau-50_test",
    "epochs_to_checkpoint": 1000,
    "epochs_to_reconstruction": 1000,
    "loss": "loss_NFGP",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 256,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [256,256,256],
        "w0": 30
    }
}
