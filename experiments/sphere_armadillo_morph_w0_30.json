{
    "description": "Solving the Eikonal equation without restrictions along time. We consider the cube as an initial condition.",
    "space_time": true,
    "num_epochs": 10001,
    "sampling_opts": {
        "samples_on_surface": 40000,
        "samples_off_surface": 40000,
        "random_surf_samples": true,
        "sampler": false
    },
    "scaling": "",
    "dataset": [["armadillo.ply", 0.0], ["sphere.ply", 0.2]],
    "off_surface_normals": [-1, -1, -1],
    "batch_size": 15000,
    "checkpoint_path": "logs",
    "experiment_name": "sphere_armadillo_morph_3x256_w0-30",
    "epochs_to_checkpoint": 1000,
    "epochs_to_reconstruction": 1000,
    "loss": "loss_mean_curv",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 128,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [256,256,256],
        "w0": 30
    }
}