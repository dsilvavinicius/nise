{
    "description": "SIREN Learning of the Armadillo mesh.",
    "space_time": true,
    "num_epochs": 50001,
    "sampling_opts": {
        "samples_on_surface": 2000,
        "samples_off_surface": 2000,
        "random_surf_samples": true,
        "sampler": false
    },
    "dataset": [["bob.ply", "shapeNets/bob_1x64_w0-16.pth", 0.0]],
    "batch_size": 2000,
    "checkpoint_path": "logs",
    "experiment_name": "bob_1x128_w0-20_vector_field",
    "epochs_to_checkpoint": 1000,
    "epochs_to_reconstruction": 1000,
    "loss": "loss_level_set",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 256,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [128,128],
        "w0": 20
    }
}
