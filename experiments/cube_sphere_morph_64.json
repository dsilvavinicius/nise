{
    "description": "Solving the Eikonal equation without restrictions along time. We consider the cube as an initial condition.",
    "space_time": true,
    "num_epochs": 10000,
    "sampling_opts": {
        "samples_on_surface": 20000,
        "samples_off_surface": 20000,
        "random_surf_samples": true,
        "sampler": false
    },
    "scaling": "",
    
    "off_surface_normals": [-1, -1, -1],
    "batch_size": 15000,
    "checkpoint_path": "logs",
    "experiment_name": "cube_sphere_morph_64",
    "epochs_to_checkpoint": 100,
    "epochs_to_reconstruction": 100,
    "loss": "loss_eikonal",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 128,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [64,64,64],
        "w0": 10
    }
}