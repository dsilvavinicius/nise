{
    "description": "SIREN Learning of the spot mesh.",
    "space_time": true,
    "num_epochs": 52000,
    "sampling_opts": {
        "samples_on_surface": 98878,
        "samples_off_surface": 98878,
        "random_surf_samples": true,
        "sampler": false
    },
    "dataset": [["dumbbell.ply", "shapeNets/dumbbell_1x64_w0-16.pth", -1.0]],
    "batch_size": 40000,
    "checkpoint_path": "logs",
    "experiment_name": "dumbell_1x128_w0-16_mean_curv",
    "epochs_to_checkpoint": 1000,
    "epochs_to_reconstruction": 1000,
    "loss": "loss_mean_curv",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 256,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [128,128],
        "w0": 16
    }
}
