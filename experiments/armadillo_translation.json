{
    "description": "Solving the Eikonal equation without restrictions along time. We consider the cube as an initial condition.",
    "space_time": true,
    "num_epochs": 1000,
    "sampling_opts": {
        "samples_on_surface": 64000,
        "samples_off_surface": 64000,
        "random_surf_samples": true,
        "sampler": false
    },
    "scaling": "",
    "dataset": "armadillo.ply",
    "off_surface_normals": [-1, -1, -1],
    "batch_size": 16000,
    "checkpoint_path": "logs",
    "experiment_name": "armadillo_sdf_translation",
    "epochs_to_checkpoint": 10,
    "epochs_to_reconstruction": 10,
    "loss": "true_sdf",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 128,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [256,256,256],
        "w0": 48
    }
}
