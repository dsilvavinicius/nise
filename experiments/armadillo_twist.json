{
    "description": "SIREN Learning of the Armadillo mesh.",
    "space_time": true,
    "num_epochs": 50001,
    "sampling_opts": {
        "samples_on_surface": 172974,
        "samples_off_surface": 172974,
        "random_surf_samples": true,
        "sampler": false
    },
    "dataset": [["armadillo.ply", "shapeNets/armadillo_2x256_w-60.pth", 0.0]],
    "batch_size": 50000,
    "checkpoint_path": "logs",
    "experiment_name": "armadillo_2x256_w-60_twist",
    "epochs_to_checkpoint": 2000,
    "epochs_to_reconstruction": 2000,
    "loss": "loss_level_set",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 256,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [256,256,256],
        "w0": 60
    }
}
