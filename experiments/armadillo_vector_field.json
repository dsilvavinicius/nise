{
    "description": "SIREN Learning of the Armadillo mesh.",
    "space_time": true,
    "num_epochs": 50001,
    "sampling_opts": {
        "samples_on_surface": 172974,
        "samples_off_surface": 172974,
        "random_surf_samples": true,
        "sampler": false
    },
    "dataset": [["armadillo.ply", "shapeNets/armadillo_2x256_w-60.pth", 0.0]],
    "batch_size": 50000,
    "checkpoint_path": "logs",
    "experiment_name": "armadillo_1x512_w-30_vector_field",
    "epochs_to_checkpoint": 1000,
    "epochs_to_reconstruction": 1000,
    "loss": "loss_level_set",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 256,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [512,512],
        "w0": 30
    }
}
