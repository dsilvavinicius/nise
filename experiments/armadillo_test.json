{
    "description": "SIREN Learning of the Armadillo mesh.",
    "space_time": true,
    "num_epochs": 52000,
    "sampling_opts": {
        "samples_on_surface": 80000,
        "samples_off_surface": 80000,
        "random_surf_samples": true,
        "sampler": false
    },
    "scaling": "",
    "dataset": [["armadillo.ply", -1]],
    "off_surface_normals": [-1, -1, -1],
    "batch_size": 20000,
    "checkpoint_path": "logs",
    "experiment_name": "armadillo_smooth_rede_4x256_w0_48_alpha_0.01",
    "epochs_to_checkpoint": 2000,
    "epochs_to_reconstruction": 2000,
    "loss": "loss_mean_curv",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 128,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [256, 256, 256, 256],
        "w0": 48
    }
}
