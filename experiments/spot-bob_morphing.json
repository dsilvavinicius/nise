{
    "description": "SIREN Learning of the Armadillo mesh.",
    "space_time": true,
    "num_epochs": 200001,
    "sampling_opts": {
        "samples_on_surface": 2000,
        "samples_off_surface": 2000,
        "random_surf_samples": true,
        "sampler": false
    },
    "dataset": [["spot.ply", -1],["bob.ply", 1]],
    "batch_size": 2000,
    "checkpoint_path": "logs",
    "experiment_name": "spot_bob_1x128_w-30_2",
    "epochs_to_checkpoint": 10000,
    "epochs_to_reconstruction": 10000,
    "loss": "loss_morphing_two_sirens",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 128,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [128,128],
        "w0": 30
    }
}
