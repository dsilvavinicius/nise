{
    "description": "SIREN Learning of the spot mesh.",
    "space_time": true,
    "num_epochs": 52000,
    "sampling_opts": {
        "samples_on_surface": 72466,
        "samples_off_surface": 72466,
        "random_surf_samples": true,
        "sampler": false
    },
    "dataset": [["falcon_sub.ply", "shapeNets/falcon_2x128_w0-30.pth", -0.5]],
    "batch_size": 35000,
    "checkpoint_path": "logs",
    "experiment_name": "falcon_2x128_w0-30_mean_curv_restrictions",
    "epochs_to_checkpoint": 1000,
    "epochs_to_reconstruction": 1000,
    "loss": "loss_mean_curv_with_restrictions",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 256,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [128,128,128],
        "w0": 30
    }
}
