{
    "description": "SIREN Learning the transport of Sphere.",
    "space_time": true,
    "num_epochs": 10000,
    "sampling_opts": {
        "samples_on_surface": 80000,
        "samples_off_surface": 80000,
        "random_surf_samples": true,
        "sampler": false
    },
    "scaling": "",
    "dataset": [["3spheres.ply", 1]],
    "off_surface_normals": [-1, -1, -1],
    "batch_size": 20000,
    "checkpoint_path": "logs",
    "experiment_name": "blobs",
    "epochs_to_checkpoint": 100,
    "epochs_to_reconstruction": 100,
    "loss": "loss_mean_curv",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 128,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [256, 256, 256],
        "w0": 20
    }
}
