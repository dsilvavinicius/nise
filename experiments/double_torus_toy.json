{
    "description": "Simple experiment to test the learning pipeline.",
    "num_epochs": 1000,
    "sampling_opts": {
        "samples_on_surface": 4096,
        "samples_off_surface": 4096,
        "random_surf_samples": true,
        "sampler": false
    },
    "scaling": "bbox",
    "dataset": "double_torus.ply",
    "off_surface_sdf": -1,
    "off_surface_normals": [-1, -1, -1],
    "batch_size": 1024,
    "checkpoint_path": "logs",
    "experiment_name": "double_torus_toy_time0",
    "epochs_to_checkpoint": 10,
    "epochs_to_reconstruction": 10,
    "loss": "sitzmann",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 128,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [16, 16],
        "w0": 5
    }
}
