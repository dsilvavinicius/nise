{
    "description": "Solving the Eikonal equation without restrictions along time. We consider the cube as an initial condition.",
    "space_time": true,
    "num_epochs": 10000,
    "sampling_opts": {
        "samples_on_surface": 45000,
        "samples_off_surface": 45000,
        "random_surf_samples": true,
        "sampler": false
    },
    "scaling": "",
    "dataset": [["armadillo.ply", 0],["cube.ply", 0.2]],
    "off_surface_normals": [-1, -1, -1],
    "batch_size": 20000,
    "checkpoint_path": "logs",
    "experiment_name": "armadillo_cube_vector_field_morphing_rede-3x256_w0-30",
    "epochs_to_checkpoint": 100,
    "epochs_to_reconstruction": 100,
    "loss": "loss_vector_field_morph",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 256,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [256,256,256,256],
        "w0": 30
    }
}